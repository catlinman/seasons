{
    // JSHint Configuration File
    // See http //jshint.com/docs/ for more details

    "maxerr": false, // {int} Maximum error before stopping

    // Enforcing
    "bitwise": false, // {boolean} Prohibit bitwise operators (&, |, ^, etc.)
    "camelcase": true, // {boolean} Identifiers must be in camelCase
    "curly": false, // {boolean} Require {} for every new block or scope
    "eqeqeq": true, // {boolean} Require triple equals (===) for comparison
    "forin": true, // {boolean} Require filtering for..in loops with obj.hasOwnProperty()
    "immed": true, // {boolean} Require immediate invocations to be wrapped in parens e.g. `(function () { } ());`
    "latedef": false, // {boolean} Require variables/functions to be defined before being used
    "newcap": true, // {boolean} Require capitalization of all constructor functions e.g. `new F()`
    "noarg": true, // {boolean} Prohibit use of `arguments.caller` and `arguments.callee`
    "noempty": false, // {boolean} Prohibit use of empty blocks
    "nonew": false, // {boolean} Prohibit use of constructors for side-effects (without assignment)
    "plusplus": false, // {boolean} Prohibit use of `++` & `--`
    "quotmark": "double", // Quotation mark consistency:
    // false    do nothing (default)
    // true     ensure whatever is used is consistent
    // "single" require single quotes
    // "double" require double quotes
    "undef": true, // {boolean} Require all non-global variables to be declared (prevents global leaks)
    "unused": false, // {boolean} Require all defined variables be used
    "strict": false, // {boolean} Requires all functions run in ES5 Strict Mode
    "maxstatements": false, // {int} Max number statements per function
    "maxcomplexity": false, // {int} Max cyclomatic complexity per function
    "maxlen": 120, // {int} Max number of characters per line
    "trailing": true, // {boolean} Prohibit trailing whitespaces
    // "indent": 4, // {int} Number of spaces to use for indentation. We ignore that for now.
    // "maxparams": 4, // {int} Max number of formal params allowed per function
    // "maxdepth": 3, // {int} Max depth of nested blocks (within functions)

    // Relaxing
    "asi": false, // {boolean} Tolerate Automatic Semicolon Insertion (no semicolons)
    "boss": false, // {boolean} Tolerate assignments where comparisons would be expected
    "debug": false, // {boolean} Allow debugger statements e.g. browser breakpoints.
    "eqnull": false, // {boolean} Tolerate use of `== null`
    "esnext": true, // {boolean} Allow ES.next (ES6) syntax (ex: `const`)
    "evil": false, // {boolean} Tolerate use of `eval` and `new Function()`
    "expr": false, // {boolean} Tolerate `ExpressionStatement` as Programs
    "funcscope": false, // {boolean} Tolerate defining variables inside control statements
    "globalstrict": false, // {boolean} Allow global "use strict" (also enables 'strict')
    "iterator": false, // {boolean} Tolerate using the `__iterator__` property
    "lastsemic": false, // {boolean} Tolerate omitting a semicolon for the last statement of a 1-line block
    "laxbreak": false, // {boolean} Tolerate possibly unsafe line breakings
    "laxcomma": false, // {boolean} Tolerate comma-first style coding
    "loopfunc": true, // {boolean} Tolerate functions being defined in loops
    "multistr": false, // {boolean} Tolerate multi-line strings
    "proto": false, // {boolean} Tolerate using the `__proto__` property
    "scripturl": false, // {boolean} Tolerate script-targeted URLs
    "smarttabs": false, // {boolean} Tolerate mixed tabs/spaces when used for alignment
    "shadow": false, // {boolean} Allows re-define variables later in code e.g. `var x=1; x=2;`
    "sub": false, // {boolean} Tolerate using `[]` notation when it can still be expressed in dot notation
    "supernew": false, // {boolean} Tolerate `new function () { ... };` and `new Object;`
    "validthis": false, // {boolean} Tolerate using this in a non-constructor function

    // Environments
    "browser": true, // Web Browser (window, document, etc)
    "couch": false, // CouchDB
    "devel": false, // Development/debugging (alert, confirm, etc)
    "dojo": false, // Dojo Toolkit
    "jquery": false, // jQuery
    "mootools": false, // MooTools
    "node": true, // Node.js
    "nonstandard": false, // Widely adopted globals (escape, unescape, etc)
    "prototypejs": false, // Prototype and Scriptaculous
    "rhino": false, // Rhino
    "worker": false, // Web Workers
    "wsh": false, // Windows Scripting Host
    "yui": false, // Yahoo User Interface

    // Legacy
    "nomen": false, // {boolean} Prohibit dangling `_` in variables
    "onevar": false, // {boolean} Allow only one `var` statement per function
    "passfail": false, // {boolean} Stop on first error
    "white": false, // {boolean} Check against strict whitespace and indentation rules

